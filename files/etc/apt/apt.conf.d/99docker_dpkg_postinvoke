APT::Update::Post-Invoke {
    "sh /etc/apt/apt.conf.d.scripts/99docker_apt_postinvoke.sh";
    "sh /etc/apt/apt.conf.d.scripts/99docker_dpkg_postinvoke.sh";
}
DPkg::Post-Invoke {
    "sh /etc/apt/apt.conf.d.scripts/99docker_apt_postinvoke.sh";
    "sh /etc/apt/apt.conf.d.scripts/99docker_dpkg_postinvoke.sh";
}
